{
	"name": "template-express",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "node ./bin/www",
		"dev": "concurrently \"pnpm run server\" \"pnpm tw\"",
		"server": "node --watch --env-file=.env ./bin/www",
		"tw": "pnpm dlx @tailwindcss/cli -i ./public/stylesheets/input.css -o ./public/stylesheets/output.css --watch",

		"populate": "node db/populate.js",
		"populate:dev": "node --env-file=.env db/populate.js"
	},
	"dependencies": {
		"@tailwindcss/cli": "^4.1.17",
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"ejs": "~2.6.1",
		"express": "~5.1.0",
		"express-validator": "^7.3.0",
		"http-errors": "~1.6.3",
		"install": "^0.13.0",
		"morgan": "~1.9.1",
		"pg": "^8.16.3",
		"tailwindcss": "^4.1.17"
	},
	"packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c",
	"devDependencies": {
		"@biomejs/biome": "2.3.5",
		"concurrently": "^9.2.1"
	}
}
